
# specify the time interval
dt_interval 0.025

# determine which variable will be perturbed
perturb_rho         True
perturb_beta        True
perturb_sigma       True

# set initial value for state variables
x 1.
y 1.2
z 1.
rho 33.
beta 2.66667
sigma 10.

# set the relative standard deviation for state variables
x_rel_std 0.1

# parameters for observation
obs_rel_std      0.1
