# specify dynamic model
dyn_model lorenz

# specify dynamic model input file
dyn_model_input ./lorenz.in

# specify data assimilation method
da_filter EnKF

# specify the end time
t_end 19.75

# specify the data assimilation interval
da_interval 0.25

# specify the ensemble samples
nsamples 50

# specify whether the system is stationary or not
stationary_flag False

# specify whether to save results
save_flag True
save_folder ./results

