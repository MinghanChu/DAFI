FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     nutFoam;

startFrom       latestTime;  

startTime       0;

stopAt          nextWrite;

endTime         <endTime>;

deltaT          1;

writeControl    runTime;

writeInterval   <writeInterval>;

purgeWrite      2;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      fixed;

timePrecision   6;

runTimeModifiable true;

